    <!ENTITY fiestapriv "http://purl.org/iot/ontology/fiesta-priv#">
    <!ENTITY xml "http://www.w3.org/XML/1998/namespace" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY geo "http://www.w3.org/2003/01/geo/wgs84_pos#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY qudt "http://data.qudt.org/qudt/owl/1.0.0/unit.owl#" >
    <!ENTITY vs "http://www.w3.org/2003/06/sw-vocab-status/ns#" >
    <!ENTITY sosa "http://www.w3.org/ns/sosa/" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
    <!ENTITY ssn "http://www.w3.org/ns/ssn/">
    <!ENTITY consent "http://purl.org/adaptcentre/openscience/ontologies/consent#">
    <!ENTITY iotlite "http://purl.oclc.org/NET/UNIS/fiware/iot-lite#" >
    <!ENTITY ns "http://creativecommons.org/ns#" >
    <!ENTITY dul "http://www.loa.istc.cnr.it/ontologies/DUL.owl#" >
    <!ENTITY time "http://www.w3.org/2006/time#">
    <!ENTITY voaf "http://purl.org/vocommons/voaf#">
