<http://purl.org/iot/ontology/fiesta-priv#>
  a owl:Ontology ;
  owl:imports <http://purl.org/iot/vocab/iot-taxonomy-lite#> ;
  dc11:contributor "Elias Tragos, UCD, Dublin"@en, "Rachit Agarwal, Inria, Paris"@en, "Tarek Elsaleh, UNIS, Surrey"@en ;
  dc11:title "FIESTA-Priv Ontology"@en ;
  dc:bibliographicCitation "R. Agarwal, D. G. Fernandez, T. Elsaleh, A. Gyrard, J. Lanza, L. Sanchez, N.Georgantas, and V. Issarny. 2016. Unified IoT Ontology to Enable Interoperability and Federation of Testbeds. In 3rd IEEE World Forum on Internet of Things. IEEE, Reston, USA, pp. 70-75"@en ;
  dc11:rights "Creative Commons Attribution 4.0 International" ;
  dc:modified "2019-10-07"^^xsd:date ;
  ns0:preferredNamespacePrefix "fiesta-priv" ;
  ns0:preferredNamespaceUri "http://purl.org/iot/ontology/fiesta-iot#" ;
  rdfs:comment """
     Please report any errors to rachit.agarwal@inria.fr
    """@en ;
